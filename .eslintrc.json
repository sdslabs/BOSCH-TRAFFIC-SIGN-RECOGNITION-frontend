{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "google",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "require-jsdoc": 0,
    "react/react-in-jsx-scope": 0,
    "react/no-unescaped-entities": 0,
    "no-invalid-this": 0,
    "react/prop-types": 0,
    "no-unused-vars": 0,
    "react/no-string-refs": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
